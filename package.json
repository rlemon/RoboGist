{
  "name": "snippet-manager",
  "version": "1.0.0",
  "description": "chrome snippet manager",
  "main": "gulpfile.js",
  "scripts": {
    "dev-build": "node versioner.js && npm run-script build",
    "build": "babel ./src --out-dir ./build &&  rsync -av --exclude '*.js' ./src/ ./build/",
    "package": "npm run-script build && zip -r ./build/webstore-package.zip ./build"
  },
  "author": "rlemon",
  "license": "",
  "dependencies": {
    "babel-cli": "^6.6.4",
    "babel-preset-es2015": "^6.5.0",
    "browserify": "^13.0.0"
  }
}
